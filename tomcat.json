{
  "id": "/Tomcat",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": "",
  "container": {
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 0,
        "labels": {
          "VIP_0": "/tomcat:8080"
        },
        "protocol": "tcp",
        "servicePort": 10000,
        "name": "tomcat"
      }
    ],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "tomcat:latest",
      "forcePullImage": true,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 1,
  "disk": 0,
  "env": {
    "LIBPROCESS_IP": "0.0.0.0",
    "LIBPROCESS_PORT": "7228",
    "SPARK_MESOS_DISPATCHER_HOST": "0.0.0.0.0"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "MESOS_HTTP",
      "path": "/"
    }
  ],
  "instances": 1,
  "labels": {
    "HAPROXY_GROUP": "external"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 4040,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "fetch": [],
  "constraints": []
}